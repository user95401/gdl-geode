{
	"geode": "1.3.5",
	"version": "v1.1.2",
	"id": "gdlteam.gdl",
	"name": "GDL",
	"developer": "GDL Team",
	"description": "",
	"early-load": true,
	"resources": {
		"sprites": [
			"resources/sprites/*.png",
			"resources/fonts/*.png",
			"resources/spritesheets/*.png"
		],
		"spritesheets": {
			"GDL_Sheet": [
				"resources/spritesheets/GDL_Sheet/*.png"
			]
		},
		"files": [
			"resources/AchievementsDesc.plist",
			"resources/*.json",
			"resources/fonts/*.fnt",
			"resources/spritesheets/*.plist"
		]
	},
	"settings": {
		"framesTranslation": {
			"name": "Texture translation",
			"description": "Enables GD sprites translation",
			"type": "bool",
			"default": true
		},
		"achievementsTranslation": {
			"name": "Achievements translation",
			"description": "Enables achievements translation",
			"type": "bool",
			"default": true
		}
	}
}